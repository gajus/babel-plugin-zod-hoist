  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/helper-plugin-utils": "^7.27.1",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "multiline-ts": "^4.0.1",
    "zod": "^4.1.13"
  },
  "description": "Hoists Zod schema definitions to the top of the file.",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@types/node": "^24.10.1",
    "babel-plugin-tester": "^12.0.0",
    "cspell": "^9.4.0",
    "eslint": "^9.39.1",
    "eslint-config-canonical": "^47.3.7",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=22"
  },
  "exports": {
    ".": {
      "import": "./dist/hoistZodSchema.js",
      "types": "./dist/hoistZodSchema.d.ts"
    }
  },
  "files": [
    "./src",
    "./dist"
  ],
  "keywords": [
    "duration",
    "human",
    "format"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/hoistZodSchema.js",
  "name": "babel-plugin-zod-hoist",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/babel-plugin-zod-hoist"
  },
  "scripts": {
    "build": "rm -fr ./dist && tsc --project tsconfig.build.json",
    "dev": "tsc --watch",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:cspell",
    "lint:cspell": "cspell './**/*.{ts,tsx}' --no-progress --gitignore",
    "lint:eslint": "eslint --color .",
    "lint:tsc": "tsc",
    "test:vitest": "vitest --run --passWithNoTests"
  },
  "type": "module",
  "types": "./dist/hoistZodSchema.d.ts",
  "version": "1.0.0"
